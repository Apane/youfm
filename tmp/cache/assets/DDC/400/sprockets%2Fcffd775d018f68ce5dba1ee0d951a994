            if (check === "picks") {
              $('#maintablebody').append(html);
              $("#loading").hide();
              $("#table").show();
              $("#table table").show();
              return interactive();
            }
          },
          error: function(jqXHR, exception) {
            $("#loading").hide();
            $("#table").show();
            $('.errorcode').show();
