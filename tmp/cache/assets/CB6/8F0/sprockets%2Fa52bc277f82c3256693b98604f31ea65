      if (window.seek === true) {
        x = e.pageX - trackBar.offsetLeft;
        curr_width = $("#trackBar").width();
        percent = x / curr_width;
        if (percent > 1) {
          percent = 1;
        }
        if (percent < 0) {
          percent = 0;
